DROP TABLE public.emas_cust_temp;

CREATE TABLE public.emas_cust_temp (
	emas_cust_temp_id int4 NOT NULL,
	cert_serial_no varchar(255) NULL,
	channel varchar(255) NULL,
	common_name varchar(255) NULL,
	country varchar(255) NULL,
	created_dt timestamp NULL,
	crl_details varchar(40000) NULL,
	crl_status int4 NULL,
	crl_status_dtl varchar(40000) NULL,
	custname varchar(255) NULL,
	email_id varchar(255) NULL,
	encoded_cert varchar(40000) NULL,
	expiry_status int4 NULL,
	expiry_status_dtl varchar(40000) NULL,
	flexone varchar(255) NULL,
	flextwo varchar(255) NULL,
	given_name varchar(255) NULL,
	initails varchar(255) NULL,
	iss_common_name varchar(255) NULL,
	iss_country varchar(255) NULL,
	iss_org varchar(255) NULL,
	iss_org_unit varchar(255) NULL,
	"location" varchar(255) NULL,
	modified_dt timestamp NULL,
	ocsp_status int4 NULL,
	ocsp_status_dtl varchar(40000) NULL,
	org varchar(255) NULL,
	org_unit varchar(255) NULL,
	originalcontent varchar(255) NULL,
	request varchar(255) NULL,
	signature_alg varchar(255) NULL,
	signature_status int4 NULL,
	signature_status_dtl varchar(40000) NULL,
	signeddata varchar(40000) NULL,
	state varchar(255) NULL,
	status varchar(255) NULL,
	sur_name varchar(255) NULL,
	transresult varchar(255) NULL,
	trust_store_status int4 NULL,
	trust_store_status_dtl varchar(40000) NULL,
	unique_id varchar(255) NULL,
	valid_from timestamp NULL,
	valid_to timestamp NULL,
	CONSTRAINT emas_cust_temp_pkey PRIMARY KEY (emas_cust_temp_id)
);
